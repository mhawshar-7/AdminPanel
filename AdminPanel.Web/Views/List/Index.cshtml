@using AdminPanel.Application.Dtos
@model IEnumerable<ProjectDto>
@{
    ViewData["Title"] = "Error";
}
<h1>Projects</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Start Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.Name</td>
                <td>@project.CreatedDate?.ToString("yyyy-MM-dd")</td>
            </tr>
        }
    </tbody>
</table>